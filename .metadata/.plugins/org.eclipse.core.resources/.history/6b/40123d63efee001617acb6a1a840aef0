
public abstract class Ticket {
	private int number;
	private int daysB4Event;
	private int price;
	public abstract int getPrice();
	public int getDays(){
		return daysB4Event;
	}
	public String toString(){
		String s="Number: "+number+", Price: "+getPrice(); 
		return s;
	}
	public Ticket(int n, int days){
		n=number;
		days=daysB4Event;
	}
}
