		for(int i=0;i<getString.length();i++){
			if(getString.substring(i, i+1).equals("."))
				System.out.print(i);;
		}
		while(getString.substring(i, i+1).equals("0")&&!getString.substring(i-1, i).equals(".")){
			System.out.println(i);
			System.out.println(getString.substring(i-1, i));
			num=num.substring(0, i);
			i--;
		}
		System.out.println(num);
